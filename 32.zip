function PATCH_LIB(lib,address,edit)
libop = gg.getRangesList(lib)[1].start
hex = string.gsub(edit,"[ ]+","")
j = 1
for i=0,string.len(hex),2 do
local tt={}
tt[1]={}
tt[1].address=libop + address
tt[1].flags=gg.TYPE_DWORD
tt[1].value="h"..string.sub(hex,j+i,i+2)
gg.setValues(tt)
address = address + 1
end
end
